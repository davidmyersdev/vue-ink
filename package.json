    "build:unsafe": "vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@writewithocto/ink": "^0.6.2",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@types/node": "^16.9.2",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.8",
    "typescript": "^4.3.2",
    "vite": "^2.5.3",
    "vue-tsc": "^0.3.0"
  },
  "files": [
    "dist",
    "src"
  ]
}
